<section id="modal-buildings" class="modal" data-keyboard="false" data-backdrop="static" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <header class="modal-header">
                <h3>Buildings</h3>
            </header>
            <div class="modal-body">
                <label for="buildings-select"></label><select name="buildings-select" id="buildings-select"></select>
