{
    "name": "na-map",
    "version": "13.6.1",
    "private": true,
    "description": "Naval Action map",
    "license": "ISC",
    "scripts": {
        "build:data:convert": "node --no-warnings --experimental-specifier-resolution=node --experimental-modules --loader ts-node/esm src/js/node/convert-api-data.ts",
        "build:data:twitter": "export $(cat .env | xargs) && node --no-warnings --experimental-specifier-resolution=node --experimental-modules --loader ts-node/esm src/js/node/update-twitter/index.ts ${BEARER_TOKEN}",
        "build:deploy-main": "       TARGET=na-map       run-s prod       build:push-prod deploy:netlify",
        "build:deploy-main-quiet": " TARGET=na-map       run-s prod-quiet build:push-prod deploy:netlify",
        "build:deploy-test": "       TARGET=na-map-test  run-s prod                       deploy:netlify-test",
        "build:deploy-test2": "      TARGET=na-map-test2 run-s prod                       deploy:netlify-test2",
        "build:load": "node --no-warnings --experimental-specifier-resolution=node --experimental-modules --loader ts-node/esm src/js/node/load-api-data.ts",
        "build:push-prod": "git add --ignore-errors . && (git diff-index --quiet HEAD || git commit -m 'build: update public') && git push --quiet gitlab",
        "clean:src:js": "find ./src/js -name \"*.ts\" -type f -regextype posix-egrep -regex \".*/(browser|common|node)/.+\" -exec bash -c 'for file in \"${1%.ts}\".js \"${1%.ts}\".js.map; do [ -e \"$file\" ] && rm -f \"$file\"; done' - '{}' ';'",
        "deploy:netlify": "export $(cat .env | xargs) && netlify deploy --prod --dir public --site ${KEY_NETLIFY}",
        "deploy:netlify-test": "export $(cat .env | xargs) && netlify deploy --prod --dir public --site ${KEY_NETLIFY_TEST}",
        "deploy:netlify-test2": "export $(cat .env | xargs) && netlify deploy --prod --dir public --site ${KEY_NETLIFY_TEST2}",
        "docs": "run-p 'docs:*'",
        "docs:js": "documentation build src/js/**.js build/**.mjs -f html -o ./doc-js",
        "docs:sass": "sassdoc src/scss -d ./doc-sass",
        "helper:check-neutral-ports": "node --experimental-specifier-resolution=node --experimental-modules --loader ts-node/esm src/helper/check-neutral-ports.ts",
        "helper:size:js": "find public/ -iname '*.js' -not -iname 'data-*.js' -exec du -bc {} + | sort -k 2 | sed 's|public/||'",
        "helper:size:map": "du -s -BK src/images/map/",
        "helper:size:public": "find public/ -iname '*.js' -exec du -bc {} + | sort -k 2 | sed 's|public/||'",
        "helper:size:css": "find public/ -iname '*.css' -exec du -bc {} + | sort -k 2 | sed 's|public/||'",
        "lint": "run-s 'lint:*'",
        "lint:css": "stylelint --config .stylelintrc \"src/scss/*.scss\"",
        "lint:ejs": "ejslint src/ejs",
        "lint:js": "eslint -c .eslintrc.json --ext .ts,.tsx src/js",
        "main:between": "yarn run build:data:twitter partial-server && (git diff-index --quiet HEAD || yarn run build:deploy-main-quiet)",
        "main:develop-test": "                run-s build:load 'build:data:* full-client'         build:deploy-test",
        "main:develop-test2": "               run-s build:load 'build:data:* full-client'         build:deploy-test2",
        "main:maintenance": "yarn --silent && run-s build:load 'build:data:* full-server'         build:deploy-main-quiet",
        "main:release": "    git pull &&      run-s build:load 'build:data:* full-client' release build:deploy-main",
        "pre-build:distances": "yarn node --max-heap-size=10000 --experimental-specifier-resolution=node --experimental-modules --loader ts-node/esm src/js/node/get-distances.ts",
        "pre-build:modules": "run-s 'pre-build:modules:**'",
        "pre-build:modules:1:get": "cp -u /mnt/Modules.zip build/ && unzip -L -u build/Modules.zip -d build/",
        "pre-build:modules:2:correct:dir": "rm -rf build/Modules/; mv build/modules build/Modules",
        "pre-build:modules:3:correct:enc": "cd build/Modules/; for FILE in *.xml; do dos2unix -m -q \"${FILE}\"; done",
        "pre-build:modules:4:correct:names": "cd build/Modules/; mv \"traders brig hull.xml\" \"trader brig hull.xml\"; mv \"agamemnon deck armor .xml\" \"agamemnon deck armor.xml\"",
        "pre-build:port-bonus": "node --experimental-specifier-resolution=node --experimental-modules --loader ts-node/esm src/js/node/convert-port-bonus.ts",
        "pre-build:tiles": "vips dzsave /mnt/na-map.tif src/images/map --depth onetile --layout google --tile-size 256 --overlap 0 --suffix .webp[Q=100,reduction-effort=6]",
        "prod": "      TS_NODE_PROJECT='webpack/tsconfig-for-webpack-config.json'            webpack --config webpack/webpack.config.ts --mode=production --color",
        "prod-quiet": "TS_NODE_PROJECT='webpack/tsconfig-for-webpack-config.json' QUIET=true webpack --config webpack/webpack.config.ts --mode=production --color",
        "release": "standard-version",
        "start": "TS_NODE_PROJECT='webpack/tsconfig-for-webpack-config.json' webpack serve --config webpack/webpack.config.ts --mode=development --color --hot --open"
    },
    "browserslist": [
        "defaults and supports es6-module and last 1 version and not ios > 0 and not safari > 0"
    ],
    "prettier": {
        "printWidth": 120,
        "tabWidth": 4
    },
