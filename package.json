{
    "name": "na-map",
    "version": "9.2.0",
    "description": "Naval Action map",
    "license": "ISC",
    "author": "",
    "main": "src/js/main",
    "scripts": {
        "build:data:convert": "build/convert-api-data.mjs",
        "build:data:twitter": "export $(cat .env | xargs) && build/update-twitter.mjs ${CONSUMER_KEY} ${CONSUMER_SECRET} ${ACCESS_TOKEN} ${ACCESS_TOKEN_SECRET}",
        "build:deploy-main": " TARGET=na-map       run-s prod build:git-push deploy:netlify",
        "build:deploy-test": " TARGET=na-map-test  run-s prod                deploy:netlify-test",
        "build:deploy-test2": "TARGET=na-map-test2 run-s prod                deploy:netlify-test2",
        "build:git-push": "git add --ignore-errors . && (git diff-index --quiet HEAD || git commit -m 'squash! update') && git push --quiet gitlab",
        "build:load": "build/load-api-data.mjs",
        "deploy:netlify": "export $(cat .env | xargs) && netlify deploy --prod --dir public --site ${KEY_NETLIFY}",
        "deploy:netlify-test": "export $(cat .env | xargs) && netlify deploy --prod --dir public --site ${KEY_NETLIFY_TEST}",
        "deploy:netlify-test2": "export $(cat .env | xargs) && netlify deploy --prod --dir public --site ${KEY_NETLIFY_TEST2}",
        "dev": "NODE_ENV=development webpack --mode=development --colors --config webpack.config.js",
        "docs": "run-p docs:*",
        "docs:js": "documentation build src/js/**.js build/**.mjs -f html -o ./doc-js",
        "docs:sass": "sassdoc src/scss -d ./doc-sass",
        "lint": "run-s lint:*",
        "lint:css": "stylelint --config .stylelintrc --syntax scss \"src/scss/*.scss\"",
        "lint:js": "eslint -c .eslintrc.json src/js",
        "main:between": "                     run-s            'build:data:twitter partial-server' build:deploy-main",
        "main:development": "                 run-s build:load 'build:data:* full-client'          build:deploy-main",
        "main:maintenance": "yarn --silent && run-s build:load 'build:data:* full-server'          build:deploy-main",
        "pre-build:modules": "run-s pre-build:modules:**",
        "pre-build:modules:correct:dir": "rm -rf build/Modules/; mv build/modules build/Modules",
        "pre-build:modules:correct:enc": "cd build/Modules/; for FILE in *.xml; do dos2unix -m -q \"${FILE}\"; done",
        "pre-build:modules:correct:names": "cd build/Modules/; mv \"traders brig hull.xml\" \"trader brig hull.xml\"; mv \"agamemnon deck armor .xml\" \"agamemnon deck armor.xml\"",
        "pre-build:modules:get": "cp -u /mnt/Modules.zip build/ && unzip -Iutf-8 -L -u build/Modules.zip -d build/",
        "pre-build:tiles": "cp /mnt/na-map.tif src/images/map.tif && rm -rf src/images/map/ && build/make-tiles.mjs src/images/map.tif && rm src/images/map.tif",
        "prettier": "prettier --tab-width 4 --print-width 120 --write .eslintrc.json package.json *.js src/js/*.json src/scss/*.scss",
        "prod": "NODE_ENV=production webpack --mode=production --colors --config webpack.config.js",
        "start": "NODE_ENV=development webpack-dev-server --open --mode=development --colors --config webpack.config.js",
        "watch": "NODE_ENV=development webpack --mode=development --watch --colors --config webpack.config.js"
    },
    "browserslist": [
        ">= 1%",
        "last 1 major version",
        "not dead",
        "not ie > 0",
        "not android > 0"
    ],
    "prettier": {
        "printWidth": 120,
        "tabWidth": 4
    },
    "dependencies": {
        "@d3fc/d3fc-label-layout": "^6.0.11",
        "Flags": "link:./src/images/flags",
        "Fonts": "link:./src/fonts",
        "Icons": "link:./src/icons",
        "bootstrap": "^4.3.1",
        "bootstrap-select": "1.13.12",
        "d3-area-label": "^1.5.0",
        "d3-array": "^2.4.0",
        "d3-axis": "^1.0.12",
        "d3-collection": "^1.0.7",
        "d3-drag": "^1.2.5",
        "d3-format": "^1.4.2",
        "d3-geo": "^1.11.9",
        "d3-interpolate": "^1.4.0",
        "d3-polygon": "^1.0.6",
        "d3-scale": "^3.2.1",
        "d3-scale-chromatic": "^1.5.0",
        "d3-selection": "1.4.1",
        "d3-shape": "^1.3.7",
        "d3-zoom": "^1.8.3",
        "hashids": "2.1.0",
        "js-cookie": "^2.2.1",
        "lit-html": "^1.1.2",
        "moment": "^2.24.0",
        "round-slider": "^1.3.3",
        "semver": "^7.1.2",
        "tablesort": "^5.1.0",
        "tempusdominus-bootstrap-4": "^5.1.2",
        "tempusdominus-core": "^5.0.3",
        "timelines-chart": "^2.8.4"
    },
    "devDependencies": {
        "@babel/core": "^7.7.5",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
        "@babel/plugin-transform-spread": "^7.7.4",
        "@babel/preset-env": "^7.7.5",
        "angular-html-parser": "^1.3.0",
        "autoprefixer": "^9.7.3",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.1.0",
        "core-js": "^3.4.7",
        "css": "^2.2.4",
        "css-loader": "^3.3.0",
        "d3-node": "^2.2.1",
        "dayjs": "^1.8.19",
        "denque": "^1.4.1",
        "diff2html-cli": "^4.2.1",
        "eslint": "^6.7.2",
        "eslint-config-prettier": "^6.7.0",
        "eslint-config-xo": "^0.27.2",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-lit": "^1.2.0",
        "eslint-plugin-prettier": "^3.1.1",
        "eslint-plugin-unicorn": "^16.0.0",
        "excel4node": "^1.7.2",
        "exports-loader": "^0.7.0",
        "extract-css-chunks-webpack-plugin": "^4.7.1",
        "favicons-webpack-plugin": "^3.0.1",
        "fibers": "^4.0.2",
        "file-loader": "^5.0.2",
        "git": "^0.1.5",
        "html-webpack-plugin": "^4.0.0-beta.11",
        "husky": "^4.2.1",
        "hyper": "^0.0.9",
        "image-webpack-loader": "^6.0.0",
        "imports-loader": "^0.8.0",
        "jquery": "^3.4.1",
        "lzma-native": "^6.0.0",
        "moment-timezone": "^0.5.26",
        "ndjson-cli": "^0.3.1",
        "netlify-cli": "2.21.0",
        "node-fetch": "^2.6.0",
        "node-sass": "^4.13.0",
        "npm-run-all": "^4.1.5",
        "object-merge-advanced": "^10.11.8",
        "pngjs": "^3.4.0",
        "polylabel": "^1.0.2",
        "postcss": "^7.0.24",
