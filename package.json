  "name": "na-topo",
  "version": "1.0.1",
  "devDependencies": {
    "@babel/core": "7.0.0-beta.34",
    "@babel/preset-env": "7.0.0-beta.34",
    "autoprefixer": "^7.2.1",
    "babel-eslint": "^8.0.3",
    "babel-loader": "8.0.0-beta.0",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-preset-minify": "^0.2.0",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.7",
    "eslint": "^4.12.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "git": "^0.1.5",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "imports-loader": "^0.7.1",
    "ndjson-cli": "^0.3.1",
    "netlify-cli": "^1.2.2",
    "node-sass": "^4.7.2",
    "postcss": "^6.0.10",
    "postcss-clean": "^1.0.3",
    "postcss-cli": "^4.1.0",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.6.1",
    "sass-loader": "^6.0.6",
    "shapefile": "^0.6.6",
    "style-loader": "^0.18.2",
    "stylelint": "^8.3.1",
    "stylelint-config-recommended-scss": "^2.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.7.0",
    "stylelint-scss": "^2.1.0",
    "svg-url-loader": "^2.2.1",
    "topojson": "^3.0.2",
    "uncss": "^0.15.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5"
  },
  "dependencies": {
    "bootstrap": "4.0.0-beta.2",
    "jquery": "1.9.1 - 3",
    "popper.js": "^1.12.9",
    "topojson-client": "^3.0.0"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "clean": "rm -rf public/",
    "css-lint": "stylelint --config .stylelintrc --syntax scss \"src/scss/*.scss\"",
    "deploy-change": "yarn run prod && yarn run change-ports && yarn run push && yarn run deploy-netlify",
    "deploy-update": "yarn run push && yarn run deploy-netlify",
    "deploy-netlify": "cd public && netlify deploy",
    "js-lint": "eslint -c .eslintrc.json src/js",
    "prettier": "prettier --tab-width 4 --print-width 120 --write .eslintrc.json package.json *.js src/js/*.json src/scss/*.scss",
    "prod": "yarn run clean && NODE_ENV=${NODE_ENV:=prod} webpack --colors --config webpack.config.js",
    "push": "build/port-data.sh push",
    "change-ports": "build/port-data.sh change",
    "watch": "yarn run clean && NODE_ENV=${NODE_ENV:=dev} webpack --watch --colors --config webpack.config.js"
  },
  "browserslist": [
    "Chrome >= 54",
    "Firefox >= 49",
    "Edge >= 13",
    "Explorer >= 11",
    "iOS >= 9.3",
    "Safari >= 9.1",
    "Android >= 4.4"
  ],
  "main": "src/js/main.js",
  "sass": "src/scss/main.scss",
  "author": "",
  "license": "ISC"
}
