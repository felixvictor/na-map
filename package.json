        "yaml": "^1.7.0",
        "yaml-unist-parser": "^1.0.0",
        "yarn-deduplicate": "^1.1.1"
    },
    "peerDependencies": {
        "jquery": "^3.4.1",
        "moment-timezone": "^0.5.14",
        "popper.js": "^1.16.0"
    },
    "engines": {
        "node": ">=12"
    },
    "sass": "src/scss/main.scss"
}
