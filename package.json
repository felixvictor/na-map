{
    "name": "na-map",
    "version": "13.3.16",
    "private": true,
    "description": "Naval Action map",
    "license": "ISC",
    "scripts": {
        "build:commit-js": "git commit -m 'build: update dist' -- ./dist || true",
        "build:data:convert": "yarn node --experimental-specifier-resolution=node dist/js/node/convert-api-data.mjs",
        "build:data:twitter": "export $(cat .env | xargs) && yarn node --experimental-specifier-resolution=node dist/js/node/update-twitter/index.mjs ${BEARER_TOKEN}",
        "build:deploy-main": "       TARGET=na-map       run-s prod       build:push-prod deploy:netlify",
        "build:deploy-main-quiet": " TARGET=na-map       run-s prod-quiet build:push-prod deploy:netlify",
        "build:deploy-test": "       TARGET=na-map-test  run-s prod                         deploy:netlify-test",
        "build:deploy-test2": "      TARGET=na-map-test2 run-s prod                         deploy:netlify-test2",
        "build:js": "run-s 'build:js:**'",
        "build:js:1:pre": "rm -rf dist",
        "build:js:2:node": "tsc --project src/js/node/tsconfig.json",
        "build:js:3:post:mv": "find ./dist/js -name \"*.js\" -type f -regextype posix-egrep -regex \".*/(common|node)/.+\" -exec bash -c 'mv \"$1\" \"${1%.js}\".mjs' - '{}' ';'",
        "build:js:4:post:rm": "find ./dist/js -name \"*.js.map\" -type f -regextype posix-egrep -regex \".*/(common|node)/.+\" -exec bash -c 'rm \"$1\"' - '{}' ';'",
        "build:load": "yarn node --experimental-specifier-resolution=node dist/js/node/load-api-data.mjs",
        "build:push-prod": "git add --ignore-errors . && (git diff-index --quiet HEAD || git commit -m 'build: update public') && git push --quiet gitlab",
        "clean:src:js": "find ./src/js -name \"*.ts\" -type f -regextype posix-egrep -regex \".*/(browser|common|node)/.+\" -exec bash -c 'for file in \"${1%.ts}\".js \"${1%.ts}\".js.map; do [ -e \"$file\" ] && rm -f \"$file\"; done' - '{}' ';'",
        "deploy:netlify": "export $(cat .env | xargs) && netlify deploy --prod --dir public --site ${KEY_NETLIFY}",
        "deploy:netlify-test": "export $(cat .env | xargs) && netlify deploy --prod --dir public --site ${KEY_NETLIFY_TEST}",
        "deploy:netlify-test2": "export $(cat .env | xargs) && netlify deploy --prod --dir public --site ${KEY_NETLIFY_TEST2}",
        "docs": "run-p 'docs:*'",
        "docs:js": "documentation build src/js/**.js build/**.mjs -f html -o ./doc-js",
        "docs:sass": "sassdoc src/scss -d ./doc-sass",
        "lint": "run-s 'lint:*'",
        "lint:css": "stylelint --config .stylelintrc --syntax scss \"src/scss/*.scss\"",
        "lint:ejs": "ejslint src/ejs",
        "lint:js": "eslint -c .eslintrc.json --ext .ts,.tsx src/js",
        "main:between": "yarn run build:data:twitter partial-server && (git diff-index --quiet HEAD || yarn run build:deploy-main-quiet)",
        "main:develop-test": "                run-s build:load build:js                 'build:data:* full-client'                  build:deploy-test",
        "main:develop-test2": "               run-s build:load build:js                 'build:data:* full-client'                  build:deploy-test2",
        "main:maintenance": "yarn --silent && run-s build:load                          'build:data:* full-server'                  build:deploy-main-quiet",
        "main:release": "    git pull &&      run-s build:load build:js build:commit-js 'build:data:* full-client'          release build:deploy-main",
        "pre-build:distances": "yarn node --max-heap-size=10000 --experimental-specifier-resolution=node dist/js/node/get-distances.mjs",
        "pre-build:distances-cut": "yarn node --max-heap-size=10000 --experimental-specifier-resolution=node dist/js/node/get-distances-cut.mjs",
        "pre-build:distances-test": "yarn node --max-heap-size=10000 --experimental-specifier-resolution=node dist/js/node/get-distances-test.mjs",
        "pre-build:modules": "run-s 'pre-build:modules:**'",
        "pre-build:modules:1:get": "cp -u /mnt/Modules.zip build/ && unzip -L -u build/Modules.zip -d build/",
        "pre-build:modules:2:correct:dir": "rm -rf build/Modules/; mv build/modules build/Modules",
        "pre-build:modules:3:correct:enc": "cd build/Modules/; for FILE in *.xml; do dos2unix -m -q \"${FILE}\"; done",
        "pre-build:modules:4:correct:names": "cd build/Modules/; mv \"traders brig hull.xml\" \"trader brig hull.xml\"; mv \"agamemnon deck armor .xml\" \"agamemnon deck armor.xml\"",
        "pre-build:port-bonus": "yarn node --experimental-specifier-resolution=node dist/js/node/convert-port-bonus.mjs",
        "pre-build:tiles": "vips dzsave /mnt/na-map.tif src/images/map --depth onetile --layout google --tile-size 256 --overlap 0 --suffix .webp[Q=100,reduction-effort=6]",
        "preprod": "rm -rf public/",
        "prod": "      TS_NODE_PROJECT='tsconfig-for-webpack-config.json'            webpack --config webpack.config.ts --mode=production --color",
        "preprod-quiet": "rm -rf public/",
        "prod-quiet": "TS_NODE_PROJECT='tsconfig-for-webpack-config.json' QUIET=true webpack --config webpack.config.ts --mode=production --color",
        "release": "standard-version",
        "size:js": "find public/ -iname '*.mjs' -not -iname 'data-*.mjs' -exec du -bc {} + | sort -k 2 | sed 's|public/||'",
        "size:map": "du -s -BK src/images/map/",
        "size:public": "find public/ -iname '*.mjs' -exec du -bc {} + | sort -k 2 | sed 's|public/||'",
        "size:css": "find public/ -iname '*.css' -exec du -bc {} + | sort -k 2 | sed 's|public/||'",
        "start": "TS_NODE_PROJECT='tsconfig-for-webpack-config.json' webpack serve --config webpack.config.ts --mode=development --color --hot --open"
    },
    "browserslist": [
        "defaults and supports es6-module and last 1 version and not ios > 0 and not safari > 0"
    ],
    "prettier": {
        "printWidth": 120,
        "tabWidth": 4
    },
